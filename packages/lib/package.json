{
  "name": "@smartnippo/lib",
  "description": "Shared utility functions for SmartNippo",
  "version": "0.1.0",
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "*",
    "jest": "*",
    "tsup": "*",
    "typescript": "*",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts"
}
